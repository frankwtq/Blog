<h1>New Article</h1>
<div class="row">
	<div class="span6 offset3">
	<%= form_for(@article) do |f| %>
		<%= f.label :title %>
		<%= f.text_field :title %>

		<%= f.label :text %>
		<%= f.text_field :text %>

		<%= f.submit "Create new article", class: "btn btn-large btn-primary" %>
	<% end %>
	</div>
</div>
